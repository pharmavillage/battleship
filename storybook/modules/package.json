{
  "name": "remote-host",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev:start",
    "dev": "npm-run-all --parallel dev:*",
    "legacy": "npm-run-all --parallel legacy:*",
    "dev:tailwind": "tailwindcss -o tailwind.css --watch",
    "prod:start": "rspack serve --config rspack.dev.js",
    "dev:start": "yarn legacy:start",
    "dev:storybook": "storybook dev -p 6006",
    "build": "npm-run-all --parallel build:*",
    "build:css": "tailwindcss -o src/tailwind.css --minify",
    "tailwind:build": "postcss -i tailwind.css -o build.css --minify",
    "legacy:start": "webpack serve --config webpack.dev.js",
    "legacy:build": "webpack --config webpack.prod.js",
    "build:rsp": "rspack --config rspack.prod.js",
    "build:storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/preset-react": "7.24.7",
    "@chromatic-com/storybook": "^1.5.0",
    "@headlessui/react": "^2.0.4",
    "@heroicons/react": "^2.1.4",
    "@module-federation/enhanced": "0.1.18",
    "@rspack/cli": "0.6.5",
    "@rspack/core": "0.6.5",
    "@rspack/dev-server": "0.6.5",
    "@storybook/addon-actions": "^8.1.10",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-interactions": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/addon-onboarding": "^8.1.9",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.3",
    "@storybook/blocks": "^8.1.9",
    "@storybook/cli": "^8.1.9",
    "@storybook/react-webpack5": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@tailwindcss/forms": "^0.5.7",
    "babel-loader": "8.3.0",
    "child_process": "^1.0.2",
    "clsx": "^2.1.1",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "5.6.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "8.4.38",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "9.5.14",
    "react": "18",
    "react-dom": "18",
    "react-router-dom": "^6.23.1",
    "storybook": "^8.1.9",
    "style-loader": "^4.0.0",
    "tailwindcss": "3.4.4",
    "tailwindcss-bg-patterns": "^0.3.0",
    "typescript": "^5.4.5",
    "webpack": "5.91.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.2"
